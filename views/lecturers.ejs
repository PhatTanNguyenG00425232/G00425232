<body>
  <h1>Lecturers</h1>
  <a href="/">Back to Home</a>
  <table border="1">
    <tr>
      <th>Lecturer ID</th>
      <th>Name</th>
      <th>Department ID</th>
      <th>Action</th>
    </tr>
    <% lecturers.forEach(lecturer => { %>
      <tr>
        <td><%= lecturer._id %></td>
        <td><%= lecturer.name %></td>
        <td><%= lecturer.did %></td>
        <td><a href="/lecturers/delete/<%= lecturer._id %>" style="color: red;">Delete</a></td>
      </tr>
    <% }); %>
  </table>
</body>